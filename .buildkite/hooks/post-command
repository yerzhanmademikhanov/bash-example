
#!/bin/bash

echo "Execution result $?"

echo "This is an example of a post-command hook from .buildkite/hooks/post-command"


echo "trying to get time elapse"

echo "TIME STARTED $(buildkite-agent meta-data get "time-started-global")"

echo "TIME FINISHED $(date +%s)"

echo `expr $(date +%s) - $(buildkite-agent meta-data get "time-started-global")`
