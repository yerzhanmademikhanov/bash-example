echo "This is an example of a pre-command hook from .buildkite/hooks/pre-command"

echo "Custom pre-command hook on pipeline."

wget "www.google.com"

echo "started running some docker stuff on pre-command"

docker run hello-world

docker ps -as | grep hello-world | docker rm $(awk '{print $1}')

echo "finished running some docker stuff on pre-command"
